<Page x:Class="BroadcastExampleApp.PageRight"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BroadcastExampleApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageRight">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Content="Right Page lower" Click="RightClicked_Lower"/>
        <Button Content="Right Page higher" Click="RightClicked_Higher" Grid.Row="1"/>
        <Button Content="Right Page Event" Click="RightClicked_Event" Grid.Row="2"/>
    </Grid>
</Page>
